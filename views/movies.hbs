<section class="moveCatalog">
  <div>
    <h2>Ironhack Cinema</h2>
    <h3>Click on the movie to check the showtimes!</h3>
  </div>
  <div class="movieList">
  {{#each this.allMovies}}

    <div class="movieBox">
      <img src="{{this.image}}" alt="Cover Picture Of {{this.title}}" />
   
    <h4>{{this.title}}</h4>
    <a href="/movies/{{this._id}}">See More</a>
 </div>
  {{/each}}
  </div>
</section>